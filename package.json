{
  "name": "ts-game-engine",
  "version": "1.0.0",
  "description": "Typescript Data Engine",
  "author": "Denis Malyavkin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manfredik/data-engine.git"
  },
  "bugs": {
    "url": "https://github.com/Manfredik/data-engine/issues"
  },
  "homepage": "https://github.com/Manfredik/data-engine#readme",
  "keywords": [],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "moduleResolution": "node",
  "scripts": {
    "setup": "yarn install",
    "lint": "yarn eslint ./src/ --ext .ts",
    "lint-fix": "yarn eslint ./src/ --ext .ts",
    "prettify": "yarn prettier --config .prettierrc ./src/**/*.ts --write",
    "build": "tsc",
    "test": "jest"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "eslint": "^8.6.0",
    "typescript": "^4.5.4",
    "tslib": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1"
  },
  "peerDependencies": {

  },
  "husky": {
    "hooks": {
      "pre-commit": "prettify && lint"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|ts)|**/__tests__/*.(js|ts)"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
